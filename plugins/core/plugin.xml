<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="edu.duke.archives.migrator.plugins.core" version="0.1.0"
        class="edu.duke.archives.migrator.plugins.Core">
    <runtime>
        <library id="core" path="classes/" type="code">
            <export prefix="*" />
        </library>
    </runtime>
    <extension-point id="MetadataManager">
        <parameter-def id="class"/>
        <parameter-def id="name"/>
        <parameter-def id="description"/>
    </extension-point>
    <extension-point id="Adapter">
        <parameter-def id="class"/>
        <parameter-def id="name"/>
        <parameter-def id="description"/>
    </extension-point>
    <extension-point id="Interface">
        <parameter-def id="class"/>
        <parameter-def id="name"/>
        <parameter-def id="description"/>
    </extension-point>
</plugin>